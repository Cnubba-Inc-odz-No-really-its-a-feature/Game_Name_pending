make:
	g++ game.cpp exeptions.cpp objectStorage.cpp operators.cpp character.cpp titlecard.cpp background.cpp button.cpp renderer.cpp card.cpp main.cpp board.cpp lane.cpp newGameButton.cpp popup.cpp -lsfml-graphics -lsfml-window -lsfml-system -lsfml-audio -o main.exe -std=c++17 -O3


run:
	make
	./main.exe

clean:
	rm main.exe
